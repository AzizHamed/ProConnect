  "version": "0.2.0",
  "configurations": [
    {
      "name": "Web",
      "request": "launch",
      "type": "node",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "web"],
      "console": "integratedTerminal"
    },
    {
      "name": "Android",
      "request": "launch",
      "type": "node",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "android"],
      "console": "integratedTerminal"
    },
    {
      "name": "Test",
      "request": "launch",
      "type": "node",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "test"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\node_modules\\expo\\AppEntry.js",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
